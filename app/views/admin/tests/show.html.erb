<h2>Test <%= @test.id %></h2>

<p><strong>Title: </strong> <%= @test.title %> </p>
<p><strong>Level: </strong> <%= test_level(@test.level) %></p>

<table>
  <tr>
    <th>Question</th>
    <th colspan="3">Action</th>
  </tr>
  <!--<= render 'questions/question', collection: @test.questions %>-->
  <%= render partial: 'questions/question', collection: @test.questions %>
</table>

<%= link_to 'New test', new_admin_test_path %> |
<%= link_to 'Back', admin_tests_path %> |
<%= link_to 'Edit', edit_admin_test_path(@test) %>