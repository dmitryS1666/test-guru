<h2><%= t('.header', number: @test.id)%></h2>

<p><strong><%= t('.title', title: @test.title) %></strong> </p>
<p><strong><%= t('.level', level: @test.level) %></strong> </p>

<table>
  <tr>
    <th>Question</th>
    <th colspan="4">Action</th>
  </tr>
  <%= render partial: 'questions/question', collection: @test.questions %>
</table>

<%= link_to t('.index_link'), admin_tests_path %> |
<%= link_to t('.edit_link'), edit_admin_test_path(@test) %>