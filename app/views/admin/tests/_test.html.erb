<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test.level) %></td>
  <td><%= test.questions.count %></td>

  <td><%= link_to t('.show_link'), admin_test_path(test) %></td>
  <td><%= link_to t('.edit_link'), edit_admin_test_path(test) %></td>
  <td>
    <%= link_to t('.delete_link'), admin_test_path(test),
                      method: :delete,
                      data: {confirm: t('.warning') } %>
  </td>
</tr>